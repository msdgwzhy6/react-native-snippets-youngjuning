'.text.md':
	# https://shields.io #
	'GitHub forks':
    'prefix': 'github-forks'
    'body': '[![GitHub forks](https://img.shields.io/github/forks/${1:username}/${2:repo}.svg?style=social&label=Fork)](${3:link})'
	'GitHub stars':
    'prefix': 'github-stars'
    'body': '[![GitHub stars](https://img.shields.io/github/stars/${1:username}/${2:repo}.svg?style=social&label=Stars)](${3:link})'
	'GitHub watchers':
    'prefix': 'github-watchers'
    'body': '[![GitHub watchers](https://img.shields.io/github/watchers/${1:username}/{2:repo}.svg?style=social&label=Watch)](${3:link})'
	'GitHub followers':
    'prefix': 'github-followers'
    'body': '[![GitHub followers](https://img.shields.io/github/followers/${1:username}.svg?style=social&label=Follow)](${2:link})'
